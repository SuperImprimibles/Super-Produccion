Sub CopiarPegarAgrupacion1()
    Dim sld As slide
    Dim shp As Shape
    Dim baseShape As Shape
    
    ' Buscar la agrupación base
    For Each sld In ActivePresentation.Slides
        For Each shp In sld.Shapes
            If shp.Type = msoGroup Then
                If shp.Name = "TERJETAS GRACIAS POR VENIR BASE" Then
                    Set baseShape = shp
                    Exit For
                End If
            End If
        Next shp
        If Not baseShape Is Nothing Then Exit For
    Next sld
    
    ' Copiar y pegar la agrupación base sobre todas las agrupaciones con el nombre "TERJETAS GRACIAS POR VENIR"
    For Each sld In ActivePresentation.Slides
        For Each shp In sld.Shapes
            If shp.Type = msoGroup Then
                If shp.Name = "TERJETAS GRACIAS POR VENIR" Then
                    baseShape.Copy
                    Set newShape = sld.Shapes.Paste(1)
                    newShape.Left = shp.Left
                    newShape.Top = shp.Top
                End If
            End If
        Next shp
    Next sld
End Sub
Sub BorrarAgrupaciones1()
    Dim sld As slide
    Dim shp As Shape
    Dim i As Integer
    
    ' Recorrer todas las diapositivas y eliminar todas las agrupaciones con el nombre "TERJETAS GRACIAS POR VENIR"
    For Each sld In ActivePresentation.Slides
        i = 1
        Do While i <= sld.Shapes.Count
            Set shp = sld.Shapes(i)
            If shp.Type = msoGroup Then
                If shp.Name = "TERJETAS GRACIAS POR VENIR" Then
                    shp.Delete
                    i = i - 1 ' Ajustar el índice después de eliminar la forma
                End If
            End If
            i = i + 1
        Loop
    Next sld
End Sub
