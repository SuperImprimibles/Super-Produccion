Sub CopiarPegarForma1()
    Dim sld As slide
    Dim shp As Shape
    Dim baseShape As Shape
    Dim newShape As Shape
    Dim origLeft As Single
    Dim origTop As Single
    Dim origWidth As Single
    Dim origHeight As Single
    
    ' Buscar la forma base
    For Each sld In ActivePresentation.Slides
        For Each shp In sld.Shapes
            If shp.Name = "INVITACION" Then
                Set baseShape = shp
                Exit For
            End If
        Next shp
        If Not baseShape Is Nothing Then Exit For
    Next sld
    
    ' Copiar y pegar la forma base sobre todas las formas con el nombre "PNG 7"
    For Each sld In ActivePresentation.Slides
        For Each shp In sld.Shapes
            If shp.Name = "INVITACION 2" Then
                ' Guarda la posición y las dimensiones originales
                origLeft = shp.Left
                origTop = shp.Top
                origWidth = shp.Width
                origHeight = shp.Height
                ' Borra la forma original "PNG 7"
                shp.Delete
                ' Copia la forma base
                baseShape.Copy
                ' Pega la forma base y la nombra como "PNG 7"
                Set newShape = sld.Shapes.Paste(1)
                newShape.Name = "INVITACION 2"
                ' Ajusta la posición y las dimensiones de la nueva forma
                newShape.Left = origLeft
                newShape.Top = origTop
                newShape.Width = origWidth
                newShape.Height = origHeight
            End If
        Next shp
    Next sld
End Sub
