Sub CrearCarpetaEnEscritorio1()
    Dim rutaCarpeta As String
    Dim nombreCarpeta As String
    Dim carpeta As Object
    Dim Diapositiva As slide
    Dim Forma As Shape
    
    ' Busca la forma con el nombre "#TEMATICA" en la diapositiva 71
    For Each Diapositiva In ActivePresentation.Slides
        If Diapositiva.SlideIndex = 71 Then
            For Each Forma In Diapositiva.Shapes
                If Forma.Name = "#TEMATICA" Then
                    ' Obtiene el texto de la forma
                    nombreCarpeta = Forma.TextFrame.TextRange.Text
                    Exit For
                End If
            Next Forma
            Exit For
        End If
    Next Diapositiva
    
    If nombreCarpeta <> "" Then
        ' Define la ruta completa de la carpeta en el escritorio
        rutaCarpeta = Environ("USERPROFILE") & "\Desktop\" & nombreCarpeta
        
        ' Crea la carpeta principal
        Set carpeta = CreateObject("Scripting.FileSystemObject")
        carpeta.CreateFolder rutaCarpeta
        
        ' Crea la carpeta "FUENTES" dentro de la carpeta principal
        carpeta.CreateFolder rutaCarpeta & "\FUENTES"
        
        ' Busca la forma con el nombre "CARPETA FUENTE" en la misma diapositiva
        For Each Forma In Diapositiva.Shapes
            If Forma.Name = "CARPETA FUENTE" Then
                ' Actualiza el texto de la forma con la ruta de la carpeta "FUENTES"
                Forma.TextFrame.TextRange.Text = rutaCarpeta & "\FUENTES"
            ElseIf Forma.Name = "CARPETA KIT" Then
                ' Actualiza el texto de la forma con la ruta de la carpeta principal
                Forma.TextFrame.TextRange.Text = rutaCarpeta
            End If
        Next Forma
    End If
End Sub
Sub CrearCarpetaEnEscritorio2()
    Dim rutaCarpeta As String
    Dim nombreCarpeta As String
    Dim carpeta As Object
    Dim Diapositiva As slide
    Dim Forma As Shape
    Dim fso As Object
    Dim sourcePath As String
    Dim destPath As String
    
    ' Define la ruta de origen de los archivos a copiar
    sourcePath = "D:\0. ARCHIVOS BASE\3. FUENTES BESES\*"
    
    ' Busca la forma con el nombre "#TEMATICA" en la diapositiva 71
    For Each Diapositiva In ActivePresentation.Slides
        If Diapositiva.SlideIndex = 71 Then
            For Each Forma In Diapositiva.Shapes
                If Forma.Name = "#TEMATICA" Then
                    ' Obtiene el texto de la forma
                    nombreCarpeta = Forma.TextFrame.TextRange.Text
                    Exit For
                End If
            Next Forma
            Exit For
        End If
    Next Diapositiva
    
    If nombreCarpeta <> "" Then
        ' Define la ruta completa de la carpeta en el escritorio
        rutaCarpeta = Environ("USERPROFILE") & "\Desktop\" & nombreCarpeta
        
        ' Crea la carpeta principal
        Set carpeta = CreateObject("Scripting.FileSystemObject")
        carpeta.CreateFolder rutaCarpeta
        
        ' Crea la carpeta "FUENTES" dentro de la carpeta principal
        carpeta.CreateFolder rutaCarpeta & "\FUENTES"
        
        ' Define la ruta de destino para los archivos a copiar
        destPath = rutaCarpeta & "\FUENTES\"
        
        ' Copia los archivos desde la ruta de origen a la carpeta "FUENTES"
        Set fso = CreateObject("Scripting.FileSystemObject")
        fso.CopyFile sourcePath, destPath
        
