Sub CambiarNombreForma1()
    Dim miDiapositiva As slide
    Dim Forma As Shape
    
        For Each miDiapositiva In ActivePresentation.Slides
        If miDiapositiva.SlideIndex = 40 Then
            For Each Forma In miDiapositiva.Shapes
                If Forma.Name = "DISEÑOTEXTO" Then
                    Forma.Name = "DISEÑOTEXTO1"
                    Exit For
                End If
            Next Forma
            Exit For
