Sub CopiaAgrupacionYPegaImagenComoRelleno()
    Dim sld As slide
    Dim shp As Shape
    Dim imgPath As String
    
    ' Ruta de la imagen de la agrupación
    imgPath = "D:\INVITACION.png"
    
    ' Itera a través de todas las diapositivas
    For Each sld In ActivePresentation.Slides
        ' Itera a través de todas las formas en la diapositiva
        For Each shp In sld.Shapes
            ' Verifica si el nombre de la forma contiene "INVITACION BASE"
            If InStr(1, shp.Name, "INVITACION BASE", vbTextCompare) > 0 Then
                ' Guarda la agrupación como imagen
                shp.Export "D:\INVITACION.png", ppShapeFormatPNG
            End If
            
            ' Verifica si el nombre de la forma contiene "INVITACION 5"
            If InStr(1, shp.Name, "INVITACION 5", vbTextCompare) > 0 Then
                ' Establece la imagen como relleno de la forma
                shp.Fill.UserPicture imgPath
            End If
        Next shp
    Next sld
    
    ' Elimina la imagen guardada
    Kill imgPath
End Sub
