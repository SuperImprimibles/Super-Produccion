Sub CopiarPegarAgrupacion30()
Dim sld As slide
    Dim shp As Shape
    Dim baseShape As Shape
    Dim newShape As Shape
    
    ' Buscar la agrupación base
    For Each sld In ActivePresentation.Slides
        For Each shp In sld.Shapes
            If shp.Type = msoGroup Then
                If shp.Name = "ETIQUETA ALFAJOR 2 BASE" Then
                    Set baseShape = shp
                    Exit For
                End If
            End If
        Next shp
        If Not baseShape Is Nothing Then Exit For
    Next sld
    
    ' Copiar y pegar la agrupación base sobre todas las agrupaciones con el nombre "TERJETAS GRACIAS POR VENIR"
    For Each sld In ActivePresentation.Slides
        For Each shp In sld.Shapes
            If shp.Type = msoGroup Then
                If shp.Name = "ETIQUETA ALFAJOR 2" Then
                    baseShape.Copy
                    Set newShape = sld.Shapes.Paste(1)
                    ' Ajustar la posición del nuevo objeto para que esté centrado
                    newShape.Left = shp.Left + (shp.Width - newShape.Width) / 2
                    newShape.Top = shp.Top + (shp.Height - newShape.Height) / 2
                End If
            End If
        Next shp
    Next sld
End Sub

Sub BorrarAgrupaciones30()
    Dim sld As slide
    Dim shp As Shape
    Dim i As Integer
    
    ' Recorrer todas las diapositivas y eliminar todas las agrupaciones con el nombre "TERJETAS GRACIAS POR VENIR"
    For Each sld In ActivePresentation.Slides
        i = 1
        Do While i <= sld.Shapes.Count
            Set shp = sld.Shapes(i)
            If shp.Type = msoGroup Then
                If shp.Name = "ETIQUETA ALFAJOR 2" Then
                    shp.Delete
                    i = i - 1 ' Ajustar el índice después de eliminar la forma
                End If
            End If
            i = i + 1
        Loop
    Next sld
End Sub
