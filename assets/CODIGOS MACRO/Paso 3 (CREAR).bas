Sub CambiarTamañoTextoFormasNombre()
    Dim miDiapositiva As slide
    Dim Forma As Shape
    Dim numDiapositiva As Integer
    Dim factorReduccion As Double
    
    ' Define el número de diapositiva a partir del cual deseas aplicar el cambio
    Const diapositivaInicio As Integer = 73
    
    ' Define el factor de reducción de tamaño de fuente (por ejemplo, 0.9 para reducir al 90%)
    factorReduccion = 1.2
    
    ' Itera a través de todas las diapositivas
    For Each miDiapositiva In ActivePresentation.Slides
        ' Obtiene el número de la diapositiva actual
        numDiapositiva = miDiapositiva.SlideIndex
        
        ' Verifica si la diapositiva está a partir de la número especificado
        If numDiapositiva >= diapositivaInicio Then
            ' Itera a través de todas las formas en la diapositiva
            For Each Forma In miDiapositiva.Shapes
                ' Verifica si el nombre de la forma es "NOMBRE"
                If Forma.Name = "NOMBRE" Then
                    ' Ajusta el tamaño de fuente proporcionalmente
                    Forma.TextFrame.TextRange.Font.Size = Forma.TextFrame.TextRange.Font.Size * factorReduccion
                End If
            Next Forma
        End If
    Next miDiapositiva
End Sub

