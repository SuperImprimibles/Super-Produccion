Sub AgruparFormas1()
    Dim Diapositiva As slide
    Dim Forma1 As Shape
    Dim Forma2 As Shape
    Dim Forma3 As Shape
    Dim Grupo As Shape

    ' Cambia el número de la diapositiva según corresponda
    Set Diapositiva = ActivePresentation.Slides(51)

    ' Cambia los nombres de las formas según corresponda
    Set Forma1 = Diapositiva.Shapes("NOMBRE")
    Set Forma2 = Diapositiva.Shapes("EDAD")
    Set Forma3 = Diapositiva.Shapes("FONDO 2")

    ' Agrupa las formas
    Set Grupo = Diapositiva.Shapes.Range(Array(Forma1.Name, Forma2.Name, Forma3.Name)).Group

    ' Opcional: Cambia el nombre del grupo
    Grupo.Name = "ENVOLTORIO PICO DULCE BASE"
End Sub
